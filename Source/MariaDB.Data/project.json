{
  "version": "1.0.1.0",
  "description": "ADO.Net driver for MariaDB and MySQL",
  "repository": {
    "type": "git",
    "url": "https://github.com/noahvans/mariadb-connector-net"
  },

  "compilationOptions": {
    "define": [ "DNXCore" ]
  },

  "namedResource": {
    "MariaDB.Data.Common.ResourceStrings": "Properties/ResourceStrings.resx"
  },
  "resource": [ "Properties/keyword.txt", "Properties/ReservedWords.txt" ],

  "dependencies": {
    "System.Data.Common": "4.0.1-*"
  },

  "frameworks": {
    "dnxcore50": {
      "dependencies": {
        "System.Security.Cryptography.Hashing.Algorithms": "4.0.0-*",
        "System.ComponentModel.TypeConverter": "4.0.1-beta-23409",
        "System.Runtime.InteropServices.RuntimeInformation": "4.0.0-*",
        "System.Collections.Specialized": "4.0.1-*",
        "System.Net.Primitives": "4.0.11-*",
        "System.Runtime.Serialization.Json": "4.0.1-*",
        "System.Net.Sockets": "4.1.0-*",
        "System.Net.NameResolution": "4.0.0-*",
        "System.IO.Pipes": "4.0.0-*",
        "System.IO.Compression": "4.0.1-*",
        "System.IO.MemoryMappedFiles": "4.0.0-*",
        "System.Security.Cryptography.X509Certificates": "4.0.0-*",
        "System.Threading.Thread": "4.0.0-*",
        "System.Runtime": "4.0.21-*",
        "System.Security.Principal.Windows": "4.0.0-*",
        "System.Threading.Timer": "4.0.1-*",
        "System.Net.Security": "4.0.0-*"
      }
    }
  }
}